server {
    listen 80;
    listen [::]:80;

    server_name {{ wsgiapp_domain_name }};

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/run/uwsgi/{{ wsgiapp_domain_name }}.sock;
    }

    location /static {
        alias {{ wsgiapp_basedir }}/{{ wsgiapp_project_name }}/static;
    }
}
